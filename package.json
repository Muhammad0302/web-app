  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/icons-material": "^5.10.16",
    "@react-leaflet/core": "^2.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "leaflet": "^1.9.3",
